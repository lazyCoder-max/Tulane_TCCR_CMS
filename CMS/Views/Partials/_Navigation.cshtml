@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage
@{
    var resources = ViewData["resources"].SafeCast<IOrderedEnumerable<Resource>>().Where(x => x.IsVisible()).OrderByDescending(y => y.CreateDate);
    var researchs = ViewData["research"].SafeCast<IOrderedEnumerable<Research>>().Where(x => x.IsVisible()).OrderByDescending(y => y.CreateDate);
    var trainingGroups = ViewData["trainingGroups"].SafeCast<IOrderedEnumerable<TrainingGroup>>().Where(x => x.IsVisible()).OrderByDescending(y => y.CreateDate);
}
<!-- main header -->
<header class="main-header style-four style-five">
    <!-- header-lower -->
    <div class="header-lower">
        <div class="outer-box">
            <div class="logo-box">
                <figure class="logo"><a href="/"><img src="../../assets/images/logo-2.png" sizes="50x50" alt=""></a></figure>
            </div>
            <div class="menu-area clearfix">
                <!--Mobile Navigation Toggler-->
                <div class="mobile-nav-toggler">
                    <i class="icon-bar"></i>
                    <i class="icon-bar"></i>
                    <i class="icon-bar"></i>
                </div>
                <nav class="main-menu navbar-expand-md navbar-light">
                    <div class="collapse navbar-collapse show clearfix" id="navbarSupportedContent">
                        <ul class="navigation clearfix">
                            <li class="">
                                <a href="/">Home</a>
                            </li>
                            <li class="dropdown">
                                <a href="index.html">Research Center</a>
                                <ul>
                                    @foreach (var research in researchs)
                                    {
                                        <li><a href="@research.Url()">@research.Title</a></li>
                                    }
                                </ul>
                            </li>
                            <li class="dropdown">
                                <a href="#">Training</a>
                                <ul>
                                    @foreach (var group in trainingGroups)
                                    {
                                        <li><a href="@group.Url()">@group.TrainingGroupTitle</a></li>
                                    }

                                </ul>
                            </li>
                            <li class="dropdown">
                                <a href="#">Resources</a>
                                <ul>
                                    @foreach (var resource in resources)
                                    {
                                        <li><a href="@resource.Url()">@resource.Name</a></li>
                                    }
                                    <li><a href="/services">Facilities and Services</a></li>
                                </ul>
                            </li>
                            <li class="">
                                <a href="/staff">Staff</a>
                            </li>
                            <li class="">
                                <a href="/about">About Us</a>
                            </li>
                        </ul>
                    </div>
                </nav>
            </div>
            <ul class="menu-right-content clearfix">
                <li class="btn-box">
                    <a href="#" class="theme-btn-one">Volunteer Research</a>
                </li>
            </ul>
        </div>
    </div>

    <!--sticky Header-->
    <div class="sticky-header">
        <div class="auto-container">
            <div class="outer-box clearfix">
                <div class="menu-area pull-left">
                    <nav class="main-menu clearfix">
                        <!--Keep This Empty / Menu will come through Javascript-->
                    </nav>
                </div>
                <ul class="menu-right-content pull-right clearfix">
                    <li class="btn-box">
                        <a href="index-4.html" class="theme-btn-one">Volunteer Research</a>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</header>
<!-- main-header end -->
<!-- Mobile Menu  -->
<div class="mobile-menu">
    <div class="menu-backdrop"></div>
    <div class="close-btn"><i class="fas fa-times"></i></div>

    <nav class="menu-box">
        <div class="nav-logo"><a href="index.html"><img src="../../assets/images/logo-2.png" alt="" title=""></a></div>
        <div class="menu-outer"><!--Here Menu Will Come Automatically Via Javascript / Same Menu as in Header--></div>
        <div class="contact-info">
            <h4>Contact Info</h4>
            <ul>
                <li>1440 Canal Street, Suit 1700, New Orleans, LA, 70112</li>
                <li><a href="tel:504-988-0200">504-988-0200</a></li>
                <li><a href="mailto:tccr@tulane.edu">tccr@tulane.edu</a></li>
            </ul>
        </div>
        <div class="social-links">
            <ul class="clearfix">
                <li><a href="https://twitter.com/TulaneCCR?s=20&t=1v7Srb8G0iSyDN15wM7qXQ"><span class="fab fa-twitter"></span></a></li>
                <li><a href="https://www.facebook.com/tulaneresearch"><span class="fab fa-facebook-square"></span></a></li>
                <li><a href="https://www.instagram.com/tulaneccr/"><span class="fab fa-instagram"></span></a></li>
            </ul>
        </div>
    </nav>
</div>
<!-- End Mobile Menu -->